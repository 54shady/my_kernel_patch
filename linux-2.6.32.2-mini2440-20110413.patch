+	mid |= sccb_read(OV9650_SCCB_ADDR, 0x1d);
+	printk("SCCB address 0x%02X, manufacture ID 0x%04X, expect 0x%04X\n", OV9650_SCCB_ADDR, mid, OV9650_MANUFACT_ID);
